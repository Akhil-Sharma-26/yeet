+              std::string result(sz, '\0');            std::string result(sz, '\0');
+              
+              // Read the whole file into the buffer.            // Read the whole file into the buffer.
+              f.read(result.data(), sz);            f.read(result.data(), sz);
+              
+              return result;            return result;
+          }        }
