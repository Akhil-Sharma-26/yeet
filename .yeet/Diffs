+              std::cout<<"ERROR::STATUS::Nothing to Compare to. Make your first commit!!"<<std::endl;            std::cout<<"ERROR::STATUS::Nothing to Compare to. Make your first commit!!"<<std::endl;
+              return;            return;
+          }        }
