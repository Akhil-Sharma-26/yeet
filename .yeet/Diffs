+                  if(! access (FilePaths[i].c_str(), X_OK)){                if(! access (FilePaths[i].c_str(), X_OK)){
+                      continue;                    continue;
+                  }                }
+              
+                  // don't show file if nothing changed                // don't show file if nothing changed
+                  if(additions == 0 && deletions == 0){                if(additions == 0 && deletions == 0){
