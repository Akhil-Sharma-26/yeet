+      std::string Author::to_stringg(){    std::string Author::to_stringg(){
+          std::string thecurrTime=timeToString(time);        std::string thecurrTime=timeToString(time);
+          return name+" <"+email+"> "+ thecurrTime;        return name+" <"+email+"> "+ thecurrTime;
+         }    }
